{
    "name": "chesszebra/lichess-oauth-server",
    "description": "An application that acts as an OAuth authorization server.",
    "type": "project",
    "homepage": "https://github.com/chesszebra/lichess-oauth-server",
    "license": "AGPL3",
    "config": {
        "sort-packages": true
    },
    "require": {
        "php": "^7.0",
        "http-interop/http-middleware": "^0.4.1",
        "league/oauth2-server": "^6.1",
        "roave/security-advisories": "dev-master",
        "symfony/console": "^3.4",
        "twig/extensions": "^1.5",
        "zendframework/zend-component-installer": "^1.0",
        "zendframework/zend-config-aggregator": "^1.0",
        "zendframework/zend-expressive": "^2.0.5",
        "zendframework/zend-expressive-helpers": "^4.0",
        "zendframework/zend-expressive-twigrenderer": "^1.4",
        "zendframework/zend-expressive-zendrouter": "^2.0.1",
        "zendframework/zend-servicemanager": "^3.3",
        "zendframework/zend-stdlib": "^3.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0.8 || ^5.7.15",
        "squizlabs/php_codesniffer": "^2.8.1",
        "zfcampus/zf-development-mode": "^3.1",
        "filp/whoops": "^2.1.7"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/App/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AppTest\\": "test/AppTest/"
        }
    }
}
