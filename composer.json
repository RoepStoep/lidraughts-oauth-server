{
    "name": "chesszebra/lichess-oauth-server",
    "description": "An application that acts as an OAuth authorization server.",
    "type": "project",
    "homepage": "https://github.com/chesszebra/lichess-oauth-server",
    "license": "AGPL3",
    "config": {
        "optimize-autoloader": true,
        "process-timeout": 5000,
        "sort-packages": true
    },
    "require": {
        "php": "^7.2",
        "ext-json": "*",
        "ext-pdo": "*",
        "http-interop/http-middleware": "^0.4.1",
        "laminas/laminas-component-installer": "^1.0",
        "laminas/laminas-config-aggregator": "^1.0",
        "laminas/laminas-servicemanager": "^3.3",
        "laminas/laminas-stdlib": "^3.1",
        "league/oauth2-server": "^7.4",
        "mezzio/mezzio": "^2.0.5",
        "mezzio/mezzio-helpers": "^4.0",
        "mezzio/mezzio-laminasrouter": "^2.0.1",
        "mezzio/mezzio-twigrenderer": "^1.4",
        "mongodb/mongodb": "1.0.*",
        "monolog/monolog": "^1.25",
        "symfony/console": "^3.4",
        "twig/extensions": "^1.5"
    },
    "require-dev": {
        "filp/whoops": "^2.7",
        "phpunit/phpunit": "^6.5",
        "roave/security-advisories": "dev-master",
        "squizlabs/php_codesniffer": "^3.5",
        "zfcampus/zf-development-mode": "^3.2"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/App/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AppTest\\": "test/AppTest/"
        }
    }
}
